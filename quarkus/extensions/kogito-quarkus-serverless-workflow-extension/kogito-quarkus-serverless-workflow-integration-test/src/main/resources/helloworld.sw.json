{  
"id": "helloworld",
"version": "1.0",
"name": "Hello World Workflow",
"description": "Inject Hello World",
"start": "Hello State",
"functions": [
    {
      "name": "concat",
      "type": "expression",
      "operation": ".result|=.+\"Hello World!\""
    }
  ],
"states":[  
