{
  "id": "rpcgreet",
  "version": "1.0",
  "name": "Greeting workflow",
  "description": "JSON based greeting workflow using grpc",
  "start": "GreetPerson",
  "functions": [
    {
      "name": "sayHello",
      "type": "rpc",
      "operation": "greeting.proto#Greeter#SayHello"
    },
    {
      "name": "doNothing",
      "type": "rpc",
      "operation": "greeting.proto#Greeter#DoNothing"
    }
  ],
  "states": [
    {
      "name": "GreetPerson",
      "type": "operation",
      "actions": [
        {
          "name": "sayHello",
          "functionRef" : {
            "refName": "sayHello",
            "arguments": {
              "name": ".name",
              "language": ".language",
              "innerHello" : {"unknown": ".unknown"}
            }
          }
        }, 
        {
          "functionRef" : "doNothing"
        }
      ],
      "end": {
        "terminate": "true"
      }
    }
  ]
}