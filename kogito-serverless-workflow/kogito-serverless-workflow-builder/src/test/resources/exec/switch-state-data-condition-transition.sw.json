{
  "id": "switch_state_data_condition_transition",
  "name": "Switch State Data Condition Transition Test",
  "expressionLang": "jsonpath",
  "version": "1.0",
  "start": "ChooseOnAge",
  "states": [
    {
      "name": "ChooseOnAge",
      "type": "switch",
      "dataConditions": [
        {
          "condition": "{{ $.[?(@.age  >= 18)] }}",
          "transition": "Approve"
        },
        {
          "condition": "{{ $.[?((@.age  >= 0) && (@.age  < 18))] }}",
          "transition": "Deny"
        }
      ],
      "defaultCondition": {
        "transition": "Deny"
      }
    },
    {
      "name": "Approve",
      "type": "inject",
      "data": {
        "decision": "Approved"
      },
      "end": true
    },
    {
      "name": "Deny",
      "type": "inject",
      "data": {
        "decision": "Denied"
      },
      "end": true
    }
  ]
}